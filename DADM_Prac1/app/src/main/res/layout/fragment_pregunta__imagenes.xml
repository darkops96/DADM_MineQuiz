<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="407dp"
    android:layout_height="661dp"
    tools:context=".Pregunta_Imagenes">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/TV_Pregunta"
        android:layout_width="353dp"
        android:layout_height="105dp"
        android:layout_marginTop="115dp"
        android:backgroundTint="#526935"
        android:fontFamily="@font/asap"
        android:text="Pregunta"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="157dp"
        android:layout_height="121dp"
        android:layout_marginTop="359dp"
        android:layout_marginEnd="132dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="117dp"
        android:layout_height="117dp"
        android:layout_marginBottom="173dp"
        android:contentDescription="Glaire"
        android:src="@drawable/glare"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="117dp"
        android:layout_height="117dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="175dp"
        android:backgroundTint="#526935"
        android:contentDescription="Allay"
        android:fontFamily="@font/asap"
        android:src="@drawable/allay"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="157dp"
        android:layout_height="121dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="171dp"
        android:backgroundTint="#526935"
        android:fontFamily="@font/asap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="157dp"
        android:layout_height="121dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="361dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="117dp"
        android:layout_height="117dp"
        android:layout_marginTop="182dp"
        android:layout_marginEnd="25dp"
        android:contentDescription="Copper Gollem"
        android:src="@drawable/golemcobre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>